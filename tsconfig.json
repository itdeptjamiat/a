{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/redux/*": [
        "./src/redux/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/services/*": [
        "./src/services/*"
        
      ],
      "@/form/*": [
        "./src/form/*"
      ],
      "@/axios/*": [
        "./src/axios/*"
      ],
      "@/typography/*": [
        "./src/typography/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
